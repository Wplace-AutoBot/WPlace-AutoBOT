{
    "name": "wplace-autobot",
    "version": "0.0.1",
    "description": "WPlace Auto-Image",
    "main": "dist/auto-image.standalone.js",
    "type": "module",
    "scripts": {
        "build": "node scripts/esbuild.config.js",
        "build:bookmarklet": "node scripts/esbuild.config.js --target=bookmarklet",
        "build:userscript": "node scripts/esbuild.config.js --target=userscript",
        "build:standalone": "node scripts/esbuild.config.js --target=standalone",
        "build:all": "node scripts/esbuild.config.js --target=all",
        "dev:userscript": "node scripts/esbuild.config.js --target=userscript --watch",
        "dev:standalone": "node scripts/esbuild.config.js --target=standalone --watch",
        "serve:dist": "http-server dist -p 5173 -c-1",
        "serve:dist:cors": "http-server dist -p 5173 -c-1 --cors",
        "tunnel:start": "node scripts/tunnel.js",
        "tunnel:quick": "cloudflared tunnel --url http://localhost:5173",
        "dev:tunnel": "run-p dev:standalone serve:dist:cors tunnel:start",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --write .",
        "format:check": "prettier --ignore-path .gitignore --ignore-path .prettierignore --ignore-unknown --check .",
        "clean": "rm -rf dist/*",
        "docs:generate": "node scripts/generate-docs.js",
        "docs:watch": "node scripts/watch-docs.js",
        "ci": "npm run format:check && npm run build:all"
    },
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@vitest/coverage-v8": "^3.2.4",
        "documentation": "^14.0.3",
        "esbuild": "^0.19.0",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "http-server": "^14.1.1",
        "jsdom": "^26.1.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.1.0"
    },
    "keywords": [
        "userscript",
        "bookmarklet",
        "wplace",
        "autobot"
    ],
    "author": "WPlace AutoBot Team",
    "license": "MIT"
}
